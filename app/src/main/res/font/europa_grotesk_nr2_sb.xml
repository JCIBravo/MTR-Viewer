        app:fontWeight="700"
        app:font="@font/europa_grotesk_nr2sb_bold" />

    <font
        android:fontStyle="italic"
        android:fontWeight="400"
        android:font="@font/europa_grotesk_nr2sb_italic"
        app:fontStyle="italic"
        app:fontWeight="400"
        app:font="@font/europa_grotesk_nr2sb_italic" />

    <font
        android:fontStyle="italic"
        android:fontWeight="700"
        android:font="@font/europa_grotesk_nr2sb_bold_italic"
